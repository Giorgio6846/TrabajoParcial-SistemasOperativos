<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Catálogo de Libros - Re:Books</title>
</head>


<header style="background-color: #333; color: #fff; text-align: center; padding: 20px;">
    <h1 style="margin: 0;">Catálogo de Libros</h1>
</header>
<%- include('../presentation_layer/partials/navigationMenu'); %>

<body>


        <% booksL.forEach(function(book) { %>
        <li>
            <a href="/book?idLibro=<%= book.idLibro %>"> <%= book.nombre %></a>
            <p>
                <%= book.precio %>
                <%= book.nombresApellidos %>
                <%= book.editorial %>
            </p>
        </li>
        <% }); %>
    

    <footer>
        <%- include('../presentation_layer/partials/footer'); %>
    </footer>

</body>

</html>
